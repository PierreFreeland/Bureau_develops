.alert-box{
  margin-bottom: 20px;
  border: 1px solid #d80039;
  padding: 10px;
  color: #d80039;
  overflow: auto;
}

.btn-middle {
  margin-top: 3px;
}

.full-button {
  width: auto !important;
}

.menu-container {
  margin: 15px 0 0 0;
  border: 1px solid <%= ENV['HUB_MAIN_COLOR_DARK'] || '#8c2e60' %>;
  overflow: hidden;
}

.menu-container:hover {
  background-color: <%= ENV['HUB_MAIN_COLOR_DARK'] || '#8c2e60' %>;
  color: #fff !important;
}

.menu-container-last {
  margin: 15px 0 0 0;
  border: 1px solid <%= ENV['HUB_MAIN_COLOR_DARK'] || '#c13856' %>;
  overflow: hidden;
}

.menu-container-last:hover {
  background-color: <%= ENV['HUB_MAIN_COLOR_DARK'] || '#c13856' %>;
}

.menu-thumb {
  float: left;
  background-color: <%= ENV['HUB_MAIN_COLOR_DARK'] || '#8c2e60' %>;
  height: 64px;
  width: 64px;
}

.menu-thumb img, .menu-thumb-last img {
  width: 42px;
  padding: 2px;
}

.menu-thumb-last {
  float: left;
  background-color: <%= ENV['HUB_MAIN_COLOR_DARK'] || '#c13856' %>;
  height: 64px;
  width: 64px;
}

.menu-thumb img, .menu-thumb-last img, .info-images img{
  display: block;
}

.menu-content,.menu-content-last {
  margin-left: 80px;
  height: 64px;
  color: #9a4873;
  font-weight: bold;
  display: flex;
  align-items: center;
  justify-content: left;
}

.menu-content:hover, .menu-content:active {
  color: #fff !important;
}
.menu-content-last {
  color: <%= ENV['HUB_MAIN_COLOR_DARK'] || '#c13856' %>;
}
.menu-content-last:hover, .menu-content-last:active {
  color: #fff !important;
}

.info-feeds {
  margin-top: 15px;
}

.info-white-bg {
  background: #FFFFFF;
  min-height: 100px;
}

.title-header-text {
  font-size: 15px;
  color: <%= ENV['HUB_MAIN_COLOR_LIGHT'] || '#5a2861' %>;
}

.topic-header {
  padding-top: 15px;
  padding-left: 10px;
  margin-bottom: 10px;
}

.info-boxes {
  padding-left: 15px;
  padding-right: 15px;
  padding-bottom: 20px;
  height: auto;
}

.info-wrapper {
  padding-top: 15px;
  overflow: hidden;
  border-top: 1px solid #eee;
}

.info-images{
  float: left;
}

.info-contents {
  margin-left: 140px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.info-body {
  font-size: 10px;
  color: grey;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.services-white-bg {
  background: #FFFFFF;
  min-height: 205px;
}

.services-panel {
  margin-top: 15px;
}

.services-title-header-text {
  color: #d80039;
  font-weight: bold;
}

.search-tab {
  border-top: 1px solid #eee;
  padding-top: 10px;
}

.search-tab-input {
  background: url(icon-search.png) no-repeat scroll 7px 7px;
  background-size: 16px;
  padding-left:30px;
  background-color: white;
  border-color: #4d4d4d;
}

.button-red {
  width: 140px;
  background: #d80039;
  color: #FFF;
  font-size: 15px;
  text-transform: uppercase;
  font-weight: 500;
  height: 30px;
  margin-left: 10px;
}

.red-frame {
  border: 1px solid #d80039;
  height: 90px;
  text-align: center;
  margin-top: 15px;
  vertical-align: middle;
  color: #d80039;
  line-height: 1.2;
  font-size: 12px;
  font-weight: bold;
}

.red-frame:hover {
  background-color: #d80039;
  color: #fff;
}

.icon-heart {
  height: 40px;
  width: 40px;
  background: url(icon-favorite-service.png) no-repeat;
  display:inline-block;
}

.red-frame:hover .icon-heart {
  background: url(icon-favorite-service-hover.png) no-repeat;
  display:inline-block;
  height: 40px;
  width: 40px;
}
.red-frame.active .icon-heart {
  background: url(icon-favorite-service-hover.png) no-repeat;
  display:inline-block;
  height: 40px;
  width: 40px;
}

.red-frame.active .icon-key-hand {
  background: url(icon-service-itg-hover.png) no-repeat;
}

.icon-key-hand {
  background: url(icon-service-itg.png) no-repeat;
  display:inline-block;
  height: 40px;
  width: 40px;
}

.red-frame:hover .icon-key-hand {
  background: url(icon-service-itg-hover.png) no-repeat;
  display:inline-block;
  height: 40px;
  width: 40px;
}

.small-text {
  font-size: 10px;
}

.large-lower-text {
  padding-top: 12px;
}

.lower-text {
  padding-top: 10px;
}

.actus {
  margin-top: 15px;
}

.actus-white-bg {
  background: #FFFFFF;
  min-height: 100px;
}

.actus-boxes {
  margin-top: 20px;
  border-top: 1px solid #eee;
  padding-top: 15px;
}

.actus-feeds {
  margin-top: -10px;
}

.actus-feeds-content {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.calendar-frame{
  width: 40px;
  background-color: #f2f2f2;
  height: 60px;
  text-align: center;
  box-shadow: 0 6px 3px -2px gray;
}

.calendar-day {
  color: #5a2a60;
  font-size: 20px;
  font-weight: bold;
}

.calendar-month {
  margin-top: -7px;
  margin-bottom: 5px;
  color: #5a2a60;
  font-size: 10px;
}

.calendar-year {
  background-color: #5a2a60;
  color: #fff;
  width: 40px;
  height: 20px;
}

.adjust-center{
  margin-left:auto;
  margin-right:auto;
  display: inline-block;
  vertical-align: middle;
  margin-top: 12px;
}

.all_link {
  margin-top:20px;
  margin-right: 10px;
}

.adjust-hover:hover {
  background-color: #eee9ef ;
}

.actus-images {
  display:block;
  float: right;
}

.disabled-links-container {
  &.menu-container {
    background: none !important;
    border-color: #c1c1c1 !important;
    .menu-thumb {
      background: #c1c1c1 !important;
      img {
        -webkit-filter: grayscale(100%); /* Safari 6.0 - 9.0 */
        filter: grayscale(100%);
      }
    }
  }
  .menu-content {
    color: #c1c1c1 !important;
    background: none !important;
  }
}

.custom-require-label:after {
color: red;
content: " *";
}
