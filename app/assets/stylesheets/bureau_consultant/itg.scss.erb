}
.span-fc-day-number.filled.half:before {
  clip: rect(0 40px 40px 20px);
}
.span-fc-day-number.filled.three-quarters:before {
  clip: rect(0 40px 40px 20px);
}
.span-fc-day-number.filled.three-quarters:after {
  clip: rect(20px 40px 40px 0);
}

