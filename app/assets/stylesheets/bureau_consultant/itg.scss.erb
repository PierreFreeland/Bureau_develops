.tunnel-container{
  float: left;
  height: 400px;
  width: 50px;
  display: flex;
}
/* ol is flex-container */
.tunnel {
  counter-reset: progress;
  padding-left: 0;
  display: flex;
  height: 100%;
  flex-direction: column;
}

/* li is flex-item and flex-container */
.tunnel li {
  display: flex;
  align-items: center;
  list-style-type: none;
  counter-increment: progress;
  flex-direction: column;
  justify-content: space-between;
	position: relative;
}

.tunnel li.is-current .border{
	display: block;
	position: absolute;
  //z-index: -1;
  top: 50%;
  left: 50%;
  right: 0px;
  bottom: 0px;
	transform: translate(-50%, -50%);
	border-radius: 50%;
	width: 20px;
	height: 20px;
  border: 5px solid $main-color-light;
}

.tunnel li.is-current .border2{
	display: block;
	position: absolute;
	//z-index: -1;
	/* top: 0px; */
	left: 50%;
	right: 0px;
	bottom: -3px;
	transform: translateX(-50%);
	border-radius: 50%;
	width: 20px;
	height: 20px;
	border: 5px solid $main-color-light;
}

/* everyone flexible except first */
.tunnel li+li {
  flex: 1 0 auto;
}

.tunnel .is-current::after{
	border: 2px solid #f2f2f2;
}

/* hide further steps */
.tunnel .is-current ~ li {
  opacity: .3;
}

/* show counter numbers */
.tunnel li::after{
	position: relative;
  content: '';
  box-sizing: border-box;
  width: 1em;
  height: 1em;
  line-height: 1em;
  background: $main-color-light;
  border-radius: 50%;
  text-align: center;
  color: #fff;
  border: 1px solid $main-color-light;
}

/* show progress bars except for first */
.tunnel li+li::before {
  content: "";
  flex: 1;
  display: block; /* IE10 fix */
  border-left: 2px solid $main-color-light;
}
.tunnel .is-current ~ li::before {
  border-left-style: dotted;
}

