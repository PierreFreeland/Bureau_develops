.span-fc-day-number.filled {
  position: relative;
  display: inline-block;
  width: 40px;
  height: 40px;
  text-align: center;
  line-height: 40px;
}
.span-fc-day-number.filled:before,
.span-fc-day-number.filled:after {
  content: " ";
  top: 0;
  left: 0;
  position: absolute;
  width: 40px;
