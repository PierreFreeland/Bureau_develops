.header-wrapper {
  position: fixed;
  width: 100%;
  z-index: 2;
}

#headerH {
  margin: 0;
  padding-left: 10px;
  right: 10px;
  padding-top: 28px;
  padding-bottom: 14px;
  position: static;

  .navbar-brand {
    height: 100%;
    margin: 0;
    padding: 0;
    margin-top: -12px;
  }

  #user-name {
    margin-right: 15px;
    padding-top: 4px;
    font-size: 13px;
    color: #592961;
  }

  #user-account {
    position: relative;
    .user-icon a {
      padding: 0;
      font-size: 22px;
      color: <%= ENV['HUB_MAIN_COLOR_LIGHT'] || '#5a2861' %>;
      line-height: 1;
    }
    > ul .popover-menu, .popover-menu {
      position: absolute;
      background: white;
      color: #592961;
      border: 1px solid #e5e5e5;
      box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
      right: 12px;
      left: auto;
      border-radius: 0;
      margin: 0;
      margin-top: 4px;
      &:after {
        position: absolute;
        right: 0;
        border-left: 8px solid transparent;
        border-bottom: 10px solid #ffffff;
        content: '';
        top: -10px;
      }
      &:before {
        position: absolute;
        right: -1px;
        border-left: 10px solid transparent;
        border-bottom: 12px solid #e5e5e5;
        content: '';
        top: -12px;
      }
      a {
        padding: 8px 15px;
        color: #592961;
        font-size: 13px;
      }
    }
  }
}

#menu {
  position: relative;
  top: 0 !important;
  width: 100%;
}

@media (max-width: 767px) {
  .stat-board {
    width: 200px;
    left: 50% !important;
    margin-left: -100px;
    .stat {
      padding: 0 !important;
    }
  }
}

.stat-board {
  left: 0;
  .stat-wrapper {
    display: inline;
  }
  .stat-wrapper-mobile {
    .stat {
      border: none;
    }
    .slick-prev, .slick-next {
      position: absolute;
      top: 20px;
      z-index: 2;
    }
    .slick-prev {
      left: 13px;
    }
    .slick-next {
      right: 13px;
    }
    .slick-next:before, .slick-prev:before {
      color: <%= ENV['HUB_MAIN_COLOR_LIGHT'] || '#f17c0f' %>;
    }
  }

  .stat-wrapper a.stat, .stat-wrapper-mobile a.stat {
    &.disabled {
      .title, .value {
        color: #c1c1c1 !important;
      }
      cursor: not-allowed;
    }
  }
}

.stat-board .stat {
  border-left: 1px solid #592961;
  padding-left: 8px;
  padding-right: 8px;
  margin-right: 0;
  @media (min-width: 1025px) and (max-width: 1050px){
    .title {
      font-size: 11px !important;
    }
    .value {
    font-size: 14px !important;
    }
  }
  @media (min-width: 1230px) {
    padding-left: 2%;
    padding-right: 2%;
  }

  .title {
    font-size: 12px;
    color: #592961;
  }
  .value {
    font-size: 15px;
  }
}

.main-content {
  padding-top: 160px;
}

#user-account .notification {
  display: inline-block;
  margin-right: 20px;
}

.notification-icon .badge {
  position: absolute;
  right: 0;
  top: 0;
  margin-right: -8px;
  margin-top: -5px;
  background-color: <%= ENV['HUB_MAIN_COLOR_LIGHT'] || '#5a2861' %>;
}

.notification .popover-menu {
  width: 340px;
  li > a {
    white-space: normal;
  }
  .block-with-circle-image-left {
    margin-bottom: 0px;
    font-size: 11px;
    color: #444444;
    .block-content {
      padding-right: 10px;
    }
  }

  .notification-item {
    border-bottom: 1px solid #f2f2f2;
    &:last-of-type {
      border: none;
    }
    .popup-close-btn {
      padding: 0;
      margin-top: -4px;
      margin-right: -4px;
      &:hover {
        background-color: transparent;
      }
    }
  }
}

