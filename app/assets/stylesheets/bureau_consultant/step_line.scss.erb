.tunnel {
  .border, .border2 {
    position: relative;
    &:after {
      content: '';
    }
    .step-text {
      position: absolute;
      top: 0;
      left: 0;
      margin-left: 30px;
      color: <%= ENV['HUB_MAIN_COLOR_DARK'] || '#8c2e60' %>;
      font-size: 12px;
      width: 60px;
    }
  }
}
