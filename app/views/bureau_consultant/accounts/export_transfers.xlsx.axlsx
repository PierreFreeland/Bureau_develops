wb = xlsx_package.workbook
wb.add_worksheet(name: "Virements") do |sheet|
  sheet.add_row ["Date", "Libell√©", "Montant", "Mode"]
  @accounts.each do |account|
    sheet.add_row [set_datetime_format(account.date),
                   account.label,
                   account.amount,
                   account.payment_type.label]
  end
end
